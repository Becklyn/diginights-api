{
    "name": "becklyn/diginights-api",
    "type": "library",
    "description": "Abstraction layer for the Diginights API",
    "homepage": "https://github.com/Becklyn/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "http://becklyn.com/"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-curl": "*",
        "201created/support": "^0.1.3"
    },
    "require-dev": {
        "becklyn/php-cs": "^1.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\DiginightsApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\DiginightsApi\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/201CREATED-GmbH/support"
        }
    ]
}
